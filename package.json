{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run css:watch\" \"vite\"",
    "css:watch": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "css:minify": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --minify",
    "build": "rm -rf dist && npm run css:minify && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@svgr/rollup": "^8.1.0",
    "@tailwindcss/cli": "^4.0.15",
    "@tailwindcss/vite": "^4.0.14",
    "axios": "^1.8.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "interweave": "^13.1.1",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tslib": "^2.8.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "tailwindcss": "^4.0.15",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
